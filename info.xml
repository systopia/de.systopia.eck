<?xml version="1.0"?>
<extension key="de.systopia.eck" type="module">
  <file>eck</file>
  <name>Entity Construction Kit</name>
  <description>Provides the ability to create and manage custom CiviCRM entities.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA GmbH</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.eck</url>
    <url desc="Documentation">https://docs.civicrm.org/eck</url>
    <url desc="Support">https://github.com/systopia/de.systopia.eck/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.1.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.82</ver>
  </compatibility>
  <requires/>
  <upgrader>CiviMix\Schema\Eck\AutomaticUpgrader</upgrader>
  <classloader>
    <psr0 prefix="CRM_" path=""/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Eck</namespace>
    <format>25.10.2</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@2.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
